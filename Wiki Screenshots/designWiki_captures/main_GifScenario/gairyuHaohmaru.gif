	<script id="LightboxHeaderTemplate" class="template" type="text/template">
		<a href="#" class="wikia-button share-button secondary">Share</a>
		{{#isUserAnon}}
		{{#imageDescription}}
		<div class="more-info-dropdown more-info-right wds-dropdown">
			<div class="wds-dropdown__toggle push-dropdown-down">
				<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 24 24" class="wds-icon wds-icon-small" id="wds-icons-question"><defs><path id="question-a" d="M12 22C6.486 22 2 17.514 2 12S6.486 2 12 2s10 4.486 10 10-4.486 10-10 10m0-22C5.383 0 0 5.383 0 12s5.383 12 12 12 12-5.383 12-12S18.617 0 12 0m0 5a4.007 4.007 0 0 0-3.771 2.666.998.998 0 1 0 1.884.668A2.001 2.001 0 0 1 14 9c0 1.104-.897 2-2 2a1 1 0 0 0-1 1v2a1 1 0 1 0 2 0v-1.127A4.006 4.006 0 0 0 16 9c0-2.205-1.794-4-4-4m-.005 11.99a1.01 1.01 0 0 0-1.006 1.005A1.01 1.01 0 0 0 11.995 19c.553 0 1.005-.453 1.005-1.005 0-.552-.452-1.005-1.005-1.005"></path></defs><use fill-rule="evenodd" xlink:href="#question-a"></use></svg>				<span>More Info</span>
			</div>
			<div class="wds-dropdown__content wds-is-right-aligned more-info-container">
				{{{imageDescription}}}
			</div>
		</div>
		{{/imageDescription}}
		{{/isUserAnon}}
		{{^isUserAnon}}
		<a href="{{fileUrl}}" class="wikia-button more-info-button secondary">More Info</a>
		{{/isUserAnon}}
		<div id="lightbox-add-to-article" class="lightbox-add-to-article">
			<button class="article-add-button secondary">Add to article</button>
			<input class="lightbox-article-input" />
		</div>

		{{#isUserAnon}}
		{{#imageDescription}}
		<div class="more-info-dropdown more-info-left wds-dropdown">
			<div class="wds-dropdown__toggle push-dropdown-down">
				<h1>{{fileTitle}}</h1>
			</div>
			<div class="wds-dropdown__content wds-is-left-aligned more-info-container">
				{{{imageDescription}}}
			</div>
		</div>
		{{/imageDescription}}
		{{^imageDescription}}
		<h1>{{fileTitle}}</h1>
		{{/imageDescription}}
		{{/isUserAnon}}
		{{^isUserAnon}}
		<h1><a href="{{fileUrl}}">{{fileTitle}}</a></h1>
		{{/isUserAnon}}
		<a href="{{rawImageUrl}}" class="see-full-size-link">See full size image</a>
		<div class="user-details caption">
			{{#caption}}<p>{{caption}}</p>{{/caption}}
			<img class="avatar" src="{{userThumbUrl}}">
			Added by <a href="{{userPageUrl}}" target="_blank">{{userName}}</a>			{{#isPostedIn}}
